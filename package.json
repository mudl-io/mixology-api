{
  "name": "frontend",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000/",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build --prefix frontend",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm install --prefix frontend",
    "predeploy": "npm run build --prefix frontend",
    "deploy": "gh-pages -d frontend/build -e frontend"
  },
  "engines": {
    "node": "14.6.0",
    "npm": "6.14.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv": "^8.2.0"
  },
  "dependencies": {
    "gh-pages": "^3.1.0"
  }
}
